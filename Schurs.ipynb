    "def appending_max(pi):\n",
    "    S=[]\n",
    "    T=[]\n",
    "    X=[]\n",
    "    for a in pi:\n",
    "        if len(a)==max([len(b) for b in pi]):\n",
    "            X.append(a)\n",
    "            for i in a:\n",
    "                S.append(i)\n",
    "    for c in pi:\n",
    "        if c not in X:\n",
    "            T.append(c)\n",
    "    return [S,T]"
   ]
  },
