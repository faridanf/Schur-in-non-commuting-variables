    "def shifted_concat(A,B):\n",
    "    'Shifted concatenation of the set partitions A and B'\n",
    "    l=0\n",
    "    c=[]\n",
    "    D=A\n",
    "    for a in D:\n",
    "        l=len(a)+l       \n",
    "    for b in B:\n",
    "        for i in b:\n",
    "            c.append(i+l)\n",
    "        D.append(c)\n",
    "        c=[]\n",
    "    return D"
   ]
