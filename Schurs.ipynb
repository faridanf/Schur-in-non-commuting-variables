    "def h_basis(n):\n",
    "    \"This function gets n and returns h_{1,2,3,...,n}\"\n",
    "    A=[]\n",
    "    for i in range(1,n+1):\n",
    "        A.append(i)\n",
    "    return h[A]"
   ]
