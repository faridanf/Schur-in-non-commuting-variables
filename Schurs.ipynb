   "source": [
    "def Steph_relabeling(pi):\n",
    "    'This gives the relabeling that Stph discovered'\n",
    "    T=pi\n",
    "    S=[]\n",
    "    while len(T)>0:\n",
    "        [R,T]=appending_max(T)\n",
    "        for i in R:\n",
    "            S.append(i)\n",
    "    return S\n",
    "        \n",
    "    "
   ]
  },
